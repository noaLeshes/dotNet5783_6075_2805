<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style TargetType="Border">
            <Setter Property="CornerRadius" Value="5"/>
        </Style>
        <local:NotshipingToVisibilityConverter 
           x:Key="NotshipingToVisibilityConverter"/>
        <local:NotDateTimeToVisibilityConverter 
           x:Key="notBooleanToVisibilityConverter"/>
        <local:convertImagePathToBitmap 
            x:Key="ImagePath" />
        <local:cartEmptyConverter 
           x:Key="checkoutVisibilityConverter"/>
        <local:cartEmptyToTextConverter 
           x:Key="checkTextConverter"/>
        <local:backgroundConverter 
           x:Key="backgroundConverter"/>
        <local:backgroundOpositeConverter 
           x:Key="backgroundOpositeConverter"/>
        <local:borderVisibility 
           x:Key="borderVisibility"/>
        <local:TextToBool 
           x:Key="TextToBool"/>
        <local:TextToBoolOpposite 
           x:Key="TextToBoolOpposite"/>
        <local:visibilToHidden 
           x:Key="visibilToHidden"/>
        <local:NameTovisibility 
           x:Key="NameTovisibility"/>
        <local:VisibilityByMail 
           x:Key="VisibilityByMail"/>
    </Application.Resources>
</Application> 
